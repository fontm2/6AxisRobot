                    </StackPanel>
                    <DockPanel Grid.Row="1" Grid.Column="0" Background="Black" Margin="0,1,1,0">
                        <Button Style="{StaticResource StopButton}" Content="Motoren Stop" FontSize="25" Height="80" Width="200" Click="Stop_Motoren_Kinect"></Button>
                    </DockPanel>
                    <DockPanel Grid.Row="0" Grid.Column="1" Grid.RowSpan="2" Background="Black" Margin="1,0,0,0">
                        <Grid Name="camera_Grid">
                            <Image Name="camera" Width="640" Height="480" />
                            <Canvas Name="canvas" Width="640" Height="480" />
                        </Grid>
                    </DockPanel>
                </Grid>
            </TabItem>
            <TabItem Header="Daten/Simulation" Name="Daten_Simulation_Tab" IsSelected="False" Style="{StaticResource CustomTabItem}">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="3*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.Resources>
                        <HierarchicalDataTemplate x:Key="UnterKnoten">
                            <!--der UnterKnoten bedarf nicht nochmal einer zuweisung einer ItemsSource, da dies bereits im ErsterKnoten unter ItemsSource dem UnterKnoten zugewiesen wurde, an Welche Quelle-->
                            <!--er gebunden wird. Hier an SubNode, eine ObservableCOllection, die Objekte aus der Klasse SubNode enthält die ein Property namens "SubNodeName" haben, an welches der Text des TextBlocks gebunden wird-->
                            <StackPanel>
                                <TextBlock FontStyle="Italic" FontSize="15" Text="{Binding Path=SubNodeName}" Foreground="White" />
                            </StackPanel>
                        </HierarchicalDataTemplate>
                        <HierarchicalDataTemplate x:Key="ErsterKnoten" ItemsSource="{Binding Path=SubNode}" ItemTemplate="{StaticResource UnterKnoten}">
                            <!--ItemsSource="{Binding Path=SubNode}" ItemsSource ist die Datenquelle die dieser ErsterKnoten als in seinen UterKnoten enthalten soll;-->
                            <!--SubNode ist eine ObservableCollection aus der Klasse TreeViewNode, die die Daten für die UnterKnoten (Objekte aus der Klasse SubNode) enthält.-->
                            <!--ItemTemplate="{StaticResource UnterKnoten}" gibt an, wie die Unterknoten aussehen sollen und welche Bindings sie haben (Text der TextBox des Unterknoten ist an das SubNodeName-Property der Objekte der Klasse SubNode gebunden-->
                            <TextBlock FontSize="15" Text="{Binding Path=NodeName}" FontWeight="Bold"  Foreground="White"/>
                        </HierarchicalDataTemplate>
                    </Grid.Resources>
                    <TreeView ItemContainerStyle="{StaticResource CustomTreeViewItem}" SelectedItemChanged="TreeViewSelectedItemChanged" Margin="0,0,1,0" Background="Black" ItemsSource="{Binding}" ItemTemplate="{StaticResource ErsterKnoten}" x:Name="DataTreeView" />
                    <!--ItemTemplate="{StaticResource ErsterKnoten}" das Template des Ersten Knotens hinzufügen, da alles andere über diesen verlinkt ist-->
                    <Grid Grid.Column="1" Grid.Row="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="3*"/>
                            <ColumnDefinition Width="1*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1*"/>
                            <RowDefinition Height="1*"/>
                        </Grid.RowDefinitions>
                        <StackPanel Margin="1,0,1,1" Grid.RowSpan="2" Grid.Column="0" Grid.Row="0" Background="Black">
                            <Label FontSize="15" Foreground="White" Content="TreeView-Daten" HorizontalAlignment="Center"></Label>
                            <StackPanel Background="Black"  Orientation="Vertical" Name="TreeViewDisplayData"></StackPanel>
                        </StackPanel>
                        <!--<StackPanel Margin="1,0,0,1" Grid.Column="1" Grid.Row="0" Background="Black"></StackPanel>
                        <StackPanel Margin="1,1,1,0" Grid.Column="0" Grid.Row="1" Background="Black"></StackPanel>
                        <StackPanel Margin="1,1,0,0" Grid.Column="1" Grid.Row="1" Background="Black"></StackPanel>-->
                        <Grid Grid.Column="1" Grid.RowSpan="2" Name="Animation" Background="Black" Margin="1,0,0,0">

                        </Grid>
                        <StackPanel Grid.Column="2" Grid.RowSpan="2" Background="WhiteSmoke">
                            <!--<Button Content="Hide Line" FontSize="15" Margin="3" Name="ShowHideLineButton" Click="ShowLines_Click"></Button>-->
                            <Slider ValueChanged="Slider_ValueChanged_XZ" Margin="3"></Slider>
                            <Slider Name="Slider_Up" Orientation="Vertical" Height="200" Width="20" ValueChanged="Slider_ValueChanged_Over" Margin="5"></Slider>
                            <Slider Name="Slider_Zoom" ValueChanged="Slider_ValueChanged_Zoom" Margin="3"></Slider>
                            <UniformGrid Rows="1" Margin="3">
                                <Label FontSize="15" Content="Winkel0"></Label>
                                <TextBox Name="Winkel0_Simu" Text="45" ></TextBox>
                            </UniformGrid>
                            <UniformGrid Rows="1" Margin="3">
                                <Label FontSize="15" Content="Winkel1"></Label>
                                <TextBox Name="Winkel1_Simu" Text="-80" ></TextBox>
                            </UniformGrid>
                            <UniformGrid Rows="1" Margin="3">
                                <Label FontSize="15" Content="Winkel2"></Label>
                                <TextBox Name="Winkel2_Simu" Text="190" ></TextBox>
                            </UniformGrid>
                            <UniformGrid Rows="1" Margin="3">
                                <Label FontSize="15" Content="Winkel3"></Label>
                                <TextBox Name="Winkel3_Simu" Text="45" ></TextBox>
                            </UniformGrid>
                            <UniformGrid Rows="1" Margin="3">
                                <Label FontSize="15" Content="Winkel4"></Label>
                                <TextBox Name="Winkel4_Simu" Text="50"></TextBox>
                            </UniformGrid>
                            <UniformGrid Rows="1" Margin="3">
                                <Label FontSize="15" Content="Winkel5"></Label>
                                <TextBox Name="Winkel5_Simu" Text="30"></TextBox>
                            </UniformGrid>
                            <!--<Button Name="AplyMotionButton" Width="120" Content="ApplyMotion" Margin="3" Click="ApplyMotion_Click"></Button>-->
                        </StackPanel>
                    </Grid>
                </Grid>
            </TabItem>
        </TabControl>
    </DockPanel>
</Window>
